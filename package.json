{
  "name": "aoc-2023",
  "version": "0.0.1",
  "description": "My Advent of Code solutions for year 2023",
  "main": "index.js",
  "scripts": {
    "build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist --minified",
    "start": "npm run build && node dist/index.js",
    "lint": "eslint --fix --ext .js ./src",
    "test": "mocha --require @babel/register src/**/*.spec.js",
    "test:watch": "mocha --require @babel/register --watch src/day-1/index.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aanykanen/aoc-2023.git"
  },
